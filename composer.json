{
    "name": "24hoursmedia/php-env-inject",
    "description": "Inject and interpolate environment variables into strings. Supports substituting default values for an env var, and escapers/formatters using callbacks. Partially supports POSIX compliant shell syntax.",
    "keywords": [
        "env",
        "environment",
        "variable",
        "var",
        "inject",
        "interpolate",
        "substitute",
        "subst",
        "replace",
        "escaper",
        "formatter",
        "posix"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "version": "dev-1.1.2",
    "autoload": {
        "psr-4": {
            "T4\\EnvInject\\": "src/"
        }
    },
    "authors": [
        {
            "name": "E.Bachman",
            "email": "e.bachman@24hoursmedia.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
